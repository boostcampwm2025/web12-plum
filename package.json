{
  "name": "gesture-recognition-video-conference",
  "version": "2.0.0",
  "description": "화상회의 + 제스처 인식 웹 애플리케이션 (NestJS 서버)",
  "scripts": {
    "install:all": "npm install --prefix client && npm install --prefix server",
    "dev:client": "npm run dev --prefix client",
    "dev:server": "npm run start:dev --prefix server",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build:client": "npm run build --prefix client",
    "build:server": "npm run build --prefix server",
    "build": "npm run build:client && npm run build:server",
    "start:client": "npm run start --prefix client",
    "start:server": "npm run start:prod --prefix server",
    "start": "concurrently \"npm run start:server\" \"npm run start:client\""
  },
  "keywords": [
    "mediapipe",
    "gesture-recognition",
    "mediasoup",
    "webrtc",
    "nestjs"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
